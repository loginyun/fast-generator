      "generatorPath": "${backendPath}/${moduleName!}/src/main/java/${packagePath}/${subModuleName!}/${ClassName}Service.java"
    },
    {
      "templateName": "ServiceImpl.java.ftl",
      "generatorPath": "${backendPath}/${moduleName!}/src/main/java/${packagePath}/impl/${subModuleName!}/${ClassName}ServiceImpl.java"
    },
    {
      "templateName": "Query.java.ftl",
      "generatorPath": "${backendPath}/${moduleName!}/src/main/java/${packagePath}/${subModuleName!}/${ClassName}Query.java"
    },
    {
      "templateName": "Entity.java.ftl",
      "generatorPath": "${backendPath}/${moduleName!}/src/main/java/${packagePath}/${subModuleName!}/${ClassName}.java"
    },
    {
      "templateName": "VO.java.ftl",
      "generatorPath": "${backendPath}/${moduleName!}/src/main/java/${packagePath}/${subModuleName!}/${ClassName}VO.java"
    },
    {
      "templateName": "Convert.java.ftl",
      "generatorPath": "${backendPath}/${moduleName!}/src/main/java/${packagePath}/${subModuleName!}/${ClassName}Convert.java"
    },
    {
      "templateName": "Dao.java.ftl",
      "generatorPath": "${backendPath}/${moduleName!}/src/main/java/${packagePath}/${subModuleName!}/${ClassName}Mapper.java"
    },
    {
      "templateName": "Dao.xml.ftl",
      "generatorPath": "${backendPath}/${moduleName!}/src/main/java/${packagePath}/${subModuleName!}/${ClassName}Mapper.xml"
    },
    {
      "templateName": "mysql.sql.ftl",
      "generatorPath": "${backendPath}/mysql.sql"
    },
    {
      "templateName": "api.ts.ftl",
      "generatorPath": "${frontendPath}/src/api/${moduleName!}/${classname}.ts"
    },
    {
      "templateName": "index.vue.ftl",
      "generatorPath": "${frontendPath}/src/views/${moduleName!}/${classname}/index.vue"
    },
    {
      "templateName": "add-or-update.vue.ftl",
      "generatorPath": "${frontendPath}/src/views/${moduleName!}/${classname}/add-or-update.vue"
    }
  ]
